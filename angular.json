{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "transport": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
                "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/transport",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              { "glob": "**/*", "input": "public" },
              { "glob": "**/*", "input": "src/assets" }
            ],
            "styles": ["src/styles.css"],
            "scripts": [],
            "prerender": false,
            "ssr": false
          },
          "configurations": {
            "production": {
              "optimization": {
                "styles": { "inlineCritical": true },
                "fonts": { "inline": false }
              },
              "budgets": [
                { "type": "initial", "maximumWarning": "700kB", "maximumError": "1.5MB" },
                { "type": "anyComponentStyle", "maximumWarning": "8kB", "maximumError": "16kB" }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": {
                "styles": { "inlineCritical": true },
                "fonts": { "inline": false }
              },
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        }
        ,
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "transport:build:production"
            },
            "development": {
              "buildTarget": "transport:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "0f0868a2-6a15-467a-af05-739c118f450f"
  }
}
